<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Optativas">

    <include
        android:id="@+id/include"
        layout="@layout/app_bar_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/editText_optativas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/menu_optativas"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="110dp"
        tools:ignore="LabelFor,MissingConstraints"
        android:importantForAutofill="no" />

    <TextView
        android:id="@+id/textView_optativas_de_inicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/op_inicio_subtit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="172dp" />

    <Switch
        android:id="@+id/switch_optativas_incio2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/op_inicio_pal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="207dp" />

    <Switch
        android:id="@+id/switch_optativas_incio5"
        android:layout_width="wrap_content"
