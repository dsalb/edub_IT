    <Switch
        android:id="@+id/switch_optativas_incio12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/op_inicio_tap"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="355dp" />

    <TextView
        android:id="@+id/op_libres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/op_libres_subtit"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="427dp" />

    <Switch
        android:id="@+id/switch_optativas_libres4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/op_libres_arq_uru"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="62dp"
