<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Perfil">

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="178dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/textView_nombre"
            android:layout_width="414dp"
            android:layout_height="wrap_content"
            android:hint="@string/nombre" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/guardar"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="678dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="57dp"
        android:layout_height="55dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/perfil"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="45dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="119dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/preferencias"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="343dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/autocad"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="419dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tresdsmax"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="477dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/illustrator"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="539dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/photoshop"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="424dp" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/revit"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="477dp" />

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/corel"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="538dp" />

</androidx.constraintlayout.widget.ConstraintLayout>

